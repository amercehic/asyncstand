{
  "name": "asyncstand",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "turbo run test",
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build --parallel",
    "build:backend": "pnpm --filter backend build",
    "build:frontend": "pnpm --filter frontend build",
    "dev": "turbo run dev --parallel",
    "start:prod": "cd apps/backend && pnpm start:prod",
    "start:staging": "cd apps/backend && pnpm start:staging",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "env:setup": "turbo run env:setup && ./scripts/setup-env.sh",
    "env:setup:force": "./scripts/setup-env.sh",
    "docker:dev": "docker-compose up",
    "docker:dev:build": "docker-compose build",
    "docker:staging": "docker-compose -f docker-compose.staging.yml up",
    "docker:staging:build": "docker-compose -f docker-compose.staging.yml build",
    "docker:prod": "docker-compose -f docker-compose.production.yml up",
    "docker:prod:build": "docker-compose -f docker-compose.production.yml build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "only-allow": "^1.2.1",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "pnpm": ">=7",
    "npm": "0",
    "yarn": "0"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^6.14.0"
  }
}

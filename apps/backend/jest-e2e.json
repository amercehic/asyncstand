{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "transformIgnorePatterns": ["node_modules/(?!.*\\.mjs$)", "../../packages/shared/dist"],
  "moduleNameMapper": {
    "^@/prisma/(.*)$": "<rootDir>/prisma/$1",
    "^@/(.*)$": "<rootDir>/src/$1"
  }
}

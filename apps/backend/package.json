{
  "name": "backend",
  "version": "1.0.1",
  "description": "NestJS backend API for AsyncStand application with authentication, user management, and audit logging",
  "main": "dist/src/main.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "clean": "rm -rf dist coverage",
    "build": "pnpm run clean && nest build",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "dev:ngrok": "cp .env.ngrok .env.local && cross-env NODE_ENV=development nest start --watch",
    "dev:debug": "cross-env NODE_ENV=development nest start --watch --debug --inspect",
    "start": "node dist/src/main.js",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main.js",
    "start:staging": "cross-env NODE_ENV=staging node dist/src/main.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "format:check": "prettier --check \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings=0",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent",
    "test:unit": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --selectProjects 'Unit Tests'",
    "test:unit:ci": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --selectProjects 'Unit Tests' --ci --coverage --reporters=default --reporters=jest-junit",
    "test:integration": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --selectProjects 'Integration Tests'",
    "test:integration:ci": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --selectProjects 'Integration Tests' --ci --coverage --reporters=default --reporters=jest-junit",
    "test:e2e": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --selectProjects 'E2E Tests'",
    "test:e2e:ci": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --selectProjects 'E2E Tests' --ci --coverage --reporters=default --reporters=jest-junit",
    "test:watch": "cross-env NODE_ENV=test jest --watch --selectProjects 'Unit Tests'",
    "test:coverage": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --coverage --selectProjects 'Unit Tests'",
    "test:coverage:all": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --coverage",
    "test:ci": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --ci --coverage --runInBand --maxWorkers=2",
    "test:changed": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --onlyChanged",
    "test:related": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --findRelatedTests",
    "test:performance": "cross-env NODE_ENV=test LOG_LEVEL=silent jest --silent --logHeapUsage --detectOpenHandles",
    "preview:email": "ts-node scripts/preview-email.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:status": "prisma migrate status",
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:seed:features": "ts-node src/features/seeds/seed-features.ts",
    "db:reset-and-seed": "prisma migrate reset --force && ts-node src/scripts/seed.ts",
    "super-admin": "ts-node src/scripts/promote-super-admin.ts"
  },
  "keywords": [
    "nestjs",
    "asyncstand",
    "authentication",
    "user-management",
    "audit-logging"
  ],
  "author": "AsyncStand Team",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asyncstand/asyncstand.git",
    "directory": "apps/backend"
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.3",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@node-rs/argon2": "^2.0.2",
    "@prisma/client": "6.15.0",
    "@slack/web-api": "^7.10.0",
    "@types/bull": "^4.10.4",
    "@types/crypto-js": "^4.2.2",
    "@types/dompurify": "^3.2.0",
    "@types/jsdom": "^21.1.7",
    "@types/nodemailer": "^6.4.19",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.6",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "jsdom": "^26.1.0",
    "nestjs-pino": "^4.4.0",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "shared": "workspace:*",
    "stripe": "^18.5.0",
    "uuid": "^11.1.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@jest/reporters": "^30.1.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mjml": "^4.7.4",
    "@types/node": "^24.3.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "husky": "^9.0.0",
    "jest": "^30.1.1",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "mjml": "^4.15.3",
    "nock": "^14.0.10",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "rimraf": "^5.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
